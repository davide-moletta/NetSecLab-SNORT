#exercise 07
#alert HTTP request with strange user agent from HOME network to 192.168.88.103

alert HTTP $HOME_NET any -> any any (
	#http_header:field user-agent;
	#pcre:"/.*Wget\/.*/";
	http_raw_header;
	pcre:"/^User-Agent:/im";	# Wget\/.*/im";
	sid:07;
	msg:"Different user agent detected in HTTP request from HOME network to client 103"
)
